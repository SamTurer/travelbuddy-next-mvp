generator client { provider = "prisma-client-js" }
datasource db { provider = "sqlite"; url = env("DATABASE_URL") }

model Place {
  id            String   @id @default(cuid())
  name          String
  category      String
  neighborhood  String?
  energyTags    String[]
  vibeTags      String[]
  durationMin   Int
  expertSnippet String?
  source        String?
  createdAt     DateTime @default(now())
}
